{
  "Apis": {
    "Default": "nbp",
    "Items": {
      "nbp": {
        "DisplayName": "NBP â€“ Narodowy Bank Polski",
        "Enabled": true
      }
    }
  },
  "Nbp": {
    "BaseUrl": "https://api.nbp.pl/",
    "TableAPath": "api/exchangerates/rates/A",
    "FormatQuery": "?format=json",
    "TablesListPath": "api/exchangerates/tables",
    "TablesForCurrencies": [ "A" ]
  },
  "Ui": {
    "DateFormat": "yyyy-MM-dd",
    "CurrencySort": "CodeAsc"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Expressions" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {SourceContext} {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "FrontendOrigin": "http://localhost:4200"
}
